<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationFindMod">

        <mods>
            <li>Vanilla Races Expanded - Android</li>
        </mods>

        <!-- Guaranteed neutroamine (unique case) -->

        <match Class="PatchOperationAddModExtension">
            <xpath>Defs/ThingDef[defName='VREA_Filth_Neutroamine']</xpath>
            <value>
                <li Class="FilthCreatesTrash.DefExtensions.GuaranteedTrashExtension">
                    <trashType>Neutroamine</trashType>
                </li>
            </value>
        </match>

    </Operation>
</Patch>